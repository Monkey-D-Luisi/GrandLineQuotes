{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Api": {
    "HttpHost": "${API_HTTP_HOST}"
  },
  "Minio": {
    "Endpoint": "${MINIO_ENDPOINT}",
    "AccessKey": "${MINIO_ROOT_USER}",
    "SecretKey": "${MINIO_ROOT_PASSWORD}",
    "Secure": "${MINIO_SECURE}",
    "Bucket": "quotes",
    "QuoteVideoPath": "videos"
  },
  "PlayIntegrity": {
    "SessionSecret": "${PLAYINTEGRITY_SESSION_SECRET}",
    "SessionMinutes": 60,
    "ServiceAccountJsonPath": "service-account.json"
  },
  "LuckyPenny": {
    "LicenseKey": "${LUCKYPENNY_LICENSE_KEY}"
  }
}
